    "github wiki to pdf",
    "wikito pdf",
    "wiki to pdf",
    "md to pdf",
    "markdown to pdf",
    "github to pdf",
    "github wiki to pdf"
  ],
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive test/unit",
